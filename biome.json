	},
	"overrides": [
		{
			"includes": ["**/src/index.ts"],
			"linter": {
				"rules": {
					"performance": {
						"noBarrelFile": "off"
					}
				}
			}
		},
		{
			"includes": ["**/*.config.*"],
			"linter": {
				"rules": {
					"correctness": {
						"noNodejsModules": "off"
					},
					"style": {
						"noDefaultExport": "off"
					}
				}
			}
		},
		{
			"includes": [
				"**/.vscode/settings.json",
				"**/*.jsonc",
				"**/tsconfig*.json"
			],
			"json": {
				"parser": {
					"allowComments": true
				}
			}
		},
		{
			"includes": ["**/*.test.*"],
			"linter": {
				"rules": {
					"style": {
						"noMagicNumbers": "off"
					}
				}
			}
		}
	],
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	}
